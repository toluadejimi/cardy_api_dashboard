{
    "name": "kenkioko/flutterwave-laravel-v3",
    "description": "A Flutterwave's Version 3 Implementation For Laravel",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "keywords": ["rave", "flutterwave", "payment", "transfers", "africa", "laravel"],
    "homepage": "https://github.com/kenkioko/flutterwave-laravel-v3",
    "autoload": {
        "classmap": [
            "library/",
            "tests/"
        ],
        "psr-4": {
            "Laravel\\Flutterwave\\": "library/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^5.6.0|^7.2",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
        "mashape/unirest-php": "^3.0|^4.0"
   },
   "require-dev": {
        "phpunit/phpunit" : "^8.3",
        "scrutinizer/ocular": "^1.1",
        "satooshi/php-coveralls": ">0.7",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^4.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Flutterwave\\RaveServiceProvider"
            ],
            "aliases": {
                "Rave": "Laravel\\Flutterwave\\Facades\\Rave"
            }
        }
    },
    "authors": [
        {
            "name": "Flutterwave Developers",
            "email": "developers@flutterwavego.com"
        },
        {
            "name": "Kennedy Kioko",
            "email": "kmkioko@gmail.com"
        }
    ]
}
